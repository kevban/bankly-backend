{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "username",
        "password",
        "firstName",
        "lastName",
        "email"
    ],
    "properties": {
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "papayaol123"
            ],
            "minLength": 6,
            "maxLength": 20
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "papaya123"
            ],
            "minLength": 6,
            "maxLength": 20
        },
        "firstName": {
            "type": "string",
            "default": "",
            "title": "The firstName Schema",
            "examples": [
                "Papaya"
            ]
        },
        "lastName": {
            "type": "string",
            "default": "",
            "title": "The lastName Schema",
            "examples": [
                "Pa"
            ]
        },
        "email": {
            "type": "string",
            "default": "",
            "title": "The email Schema",
            "examples": [
                "papaya.pa@gmail.com"
            ]
        }
    },
    "examples": [{
        "username": "papayaol123",
        "password": "papaya123",
        "firstName": "Papaya",
        "lastName": "Pa",
        "email": "papaya.pa@gmail.com"
    }]
}